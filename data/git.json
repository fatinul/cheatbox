  "Setup & Configuration": [
    {
      "command": "git config --global user.name 'Your Name'",
      "description": "Set your global git username"
    },
    {
      "command": "git config --global user.email 'your@email.com'",
      "description": "Set your global git email"
    },
    {
      "command": "git config --global core.editor vim",
      "description": "Set default text editor"
    },
    {
      "command": "git config --list",
      "description": "Show all git configurations"
    }
